# LAYER_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
LCONF_VERSION = "6"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  REPLACE/meta \
  REPLACE/meta-hob \
  REPLACE/meta-yocto \
  REPLACE/meta-yocto-bsp \
  REPLACE/meta-sunxi  \
  REPLACE/meta-cubie-ds \
  "

BBLAYERS_NON_REMOVABLE ?= " \
  REPLACE/meta \
  REPLACE/meta-yocto \
  REPLACE/meta-sunxi \
  REPLACE/meta-cubie-ds \
  "


